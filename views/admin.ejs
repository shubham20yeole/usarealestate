                                    <th>Phone</th>
                                    <th>Email</th>
                                    <th>Type</th>
                                </tr>
                            </thead>
                            <tbody id="admins">
                            <% admin.forEach(function(admin){%>
                                <tr class="a-td-<%= admin._id %>">
                                    <td><p id="a-name-<%= admin._id %>"><%= admin.fullname%></p></td>
                                    <td><p id="a-phone-<%= admin._id %>"><%= admin.phone %></p></td>
                                    <td><p id="a-email-<%= admin._id %>"><%= admin.email %></p></td>
                                    <td><a class="admintype" id="<%= admin._id %>"><%= admin.type %></td>
                                </tr>
                                <%}) %>
                            </tbody>
                        </table>

                    </div>
                    <div class="col-md-6">
                        <h5>USERS TABLE</h5>
                        <div class="table-responsive">
                            <table class="table">
