                                    <th>Phone</th>
                                    <th>Email</th>
                                    <th>Type</th>
                                </tr>
                            </thead>
                            <tbody id="users">
                            <% user.forEach(function(user){%>
                             <tr class="u-td-<%= user._id %>">
                                <td><p id="u-name-<%= user._id %>"><%= user.fullname%></p></td>
                                <td><p id="u-phone-<%= user._id %>"><%= user.phone %></p></td>
                                <td><p id="u-email-<%= user._id %>"><%= user.email %></p></td>
                                <td><a class="usertype" id="<%= user._id %>"><%= user.type %></td>
