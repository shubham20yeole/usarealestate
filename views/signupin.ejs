<div class="main"> 
<h1>REGISTER / LOGIN / RESET PASSWORD</h1>
<h1><a href='/'>GO BACK TO HOME</a></h1>
<h2 id="errmsg" style="color: #006455; font-size: 2em; text-align: center;"><%= errmsg %></h2>
<h3></h3>
						<form method="POST" action="adduser" autocomplete="off" enctype="multipart/form-data">
					    <input type="file" name="file" id="file" style="visibility:hidden;" required="">
	<div class="wthree_top_forms">
			<div class="agile-info_w3ls hvr-buzz-out">
				<h3>Register</h3>
				<div class="agile-info_w3ls_grid">
						<img src="images/defaultprofile.jpg" id="image" width="40" height="40"><br><br>
					    <button id="img">Select Profile Photo</button>"><p id="photoerrmsg"></p><br>

					    <input type="hidden" name="photo" id="photolink">
					    <input type="hidden" name="timestamp" id="timestamp">
					    <input type="hidden" name="photoname" id="photoname">
					    <input type="text" id="fullname" name="firstname" value="" placeholder="Full Name"><br>
					    <input type="email" id="remail" name="email" placeholder="Email address">
					    <input type="text" id="rphone" value="" class="halflogintext" name="phone" placeholder="Phone Number"><br><br>
					    <input type="hidden" class="logintext" name="website" value="" placeholder="Website">
					    <input type="hidden" name="gender" value="male" placeholder="Gender">
					    <input type="password" id="psd" name="password" placeholder="Password"><br><br>
					    <input type="password" id="cpsd" name="confirmpassword" placeholder="Confirm Password">
					    <input class="crhad" id="regsubmit" style="display:none;" type="submit" value="submit">
					</form>
					<br><br>
 					  <p class="fakesubmit">Submit</p>

				</div>
			</div>
			<div class="agile-info_w3ls agile-info_w3ls_sub hvr-buzz-out">
				<h3>Login</h3>
				<div class="agile-info_w3ls_grid second">
  					<form method="POST" action="/login">
      				    <input type="hidden" name="preurl" class="preurl"><br><br>
						<input type="email" placeholder="Enter Username" name="email" required>
      					<input type="password" placeholder="Enter Password" name="password" required>
					
					<div class="agile_remember">
						<div class="agile_remember_left">
							<div class="check">
								<label class="checkbox"><input type="checkbox" name="checkbox"><i> </i>remember me</label>
							</div>
						</div>
						<div class="agile_remember_right">
							<a href="#">Forgot Password?</a>
						</div>
						<div class="clear"> </div>
					</div>
						<input type="submit" value="Sign In">
					</form>
					<h4>Continue With</h4>
					<div class="social_icons agileinfo">
					   <section class="social">    
					   <% include partials/fboauth %>
					   <br><br><p style="font-size: 23px; color: white">Login with Facebook</p>
						</section>
					</div>
					<h5>Don't have an account? <a href="#">Sign Up</a></h5>
				</div>
			</div>
			<div class="agile-info_w3ls third hvr-buzz-out">
				<h3>Reset Password</h3>
				<div class="agile-info_w3ls_grid third">
								<h1>Reset Password</h1>

					<form action="/resetpassword" method="post">
						<input type="email" name="email" placeholder="Enter email address" required=" ">
						
						<p class="asd"><a href="#">Great passwords</a> use upper and lower case
							characters, numbers and symbols like "!@#$*.</p>
						<input type="submit" value="RESET">
					</form>
					<h5 class="rest">Already a member? <a href="#">Sign In</a></h5><br><br>
				</div>
			</div>
			<div class="clear"> </div>
		</div>

