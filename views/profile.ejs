  <div class="w3-row">
    <!-- Left Column -->
    <div class="w3-col m3">
      <!-- Profile -->
      <div class="w3-card-2 w3-round w3-white">
        <div class="w3-container">
         <h4 class="w3-center">My Profile</h4>
         <p class="w3-center"><img src="<%=users.photo%>" id="profile_image" class="w3-circle" style="height:106px;width:106px" alt="Avatar"></p>
         <center>
          <form method="post" action="/updatedp" enctype="multipart/form-data">
            <input type="file" name="file" id="dp"><br>
            <input type="submit" value="Upload">
          </form>
         </center>
         <hr>
         <p><i class="fa fa-pencil fa-fw w3-margin-right w3-text-theme"></i> <%=users.fullname%></p>
         <p><i class="fa fa-home fa-fw w3-margin-right w3-text-theme"></i><%=users.email%></p>
         <p><i class="fa fa-birthday-cake fa-fw w3-margin-right w3-text-theme"></i><%=users.phone%></p>
        </div>
      </div>
      <br>
      
      <!-- Accordion -->
      <div class="w3-card-2 w3-round">
        <div class="w3-accordion w3-white">
          <button onclick="myFunction('Demo1')" class="w3-btn-block w3-theme-l1 w3-left-align"><i class="fa fa-circle-o-notch fa-fw w3-margin-right"></i>SUBSCRIPTION PLANS</button>
          <div id="Demo1" class="w3-accordion-content w3-container">
            <p><br></p>
            <p>Your subscription plan: $<%=users.subscription%>/year</p>
            <p>Adds allowed: <%=users.addallowed%></p>
            <p>Adds posted: <%=users.addposted%></p>
            <p>Posting status: <%=users.poststatus%></p>
            <p><br></p>
          </div>
          <button onclick="myFunction('Demo2')" class="w3-btn-block w3-theme-l1 w3-left-align"><i class="fa fa-calendar-check-o fa-fw w3-margin-right"></i>BOOKMARKS</button>
          <div id="Demo2" class="w3-accordion-content w3-container">
          <p class="refresh" style="text-align: center;"></p>
            <% bookmark_a.forEach(function(bookmark_a){%>
            <table width="100%">
              <tbody>
                <tr class="trbookmark" id="removethis-<%=bookmark_a._id%>">
                  <td width="10%"><img src="<%=bookmark_a.image%>" width="100px" heigth="100px"><br><br></td>
                  <td width="80%"><%=bookmark_a.title%><br>$<%=bookmark_a.cost%><br><a href="<%=bookmark_a.propertyurl%>" target="_blank" class="w3-margin-left colorlink">VIEW MORE</a></td>
                  <td width="10%"><p id="<%=bookmark_a._id%>" class="removebookmark"><input type="hidden" id="t-<%=bookmark_a._id%>" value="<%=bookmark_a.timestamp%>"><i class="fa fa-times w3-margin-right links"></i></p></td>
                </tr>
              </tbody>
            </table>
            <%}) %>
          </div>
          <button onclick="myFunction('Demo3')" class="w3-btn-block w3-theme-l1 w3-left-align"><i class="fa fa-users fa-fw w3-margin-right"></i>PREVIOUS POSTS</button>
          <div id="Demo3" class="w3-accordion-content w3-container">
         <div class="w3-row-padding">
          <p class="refresh1" style="text-align: center;"></p>
        <% pastproperty.forEach(function(pastproperty){%>
            <table width="100%">
              <tbody>
                <tr class="trbookmark" id="removethis1-<%=pastproperty._id%>">
                  <td width="10%"><img src="<%=pastproperty.image1%>" width="100px" heigth="100px"><br><br></td>
                  <td width="80%"><%=pastproperty.title%><br>$<%=pastproperty.cost%><br><a href="https://shubham-great-livings.herokuapp.com/detailedproperty/<%=pastproperty.timestamp%>" target="_blank" class="w3-margin-left colorlink">VIEW MORE</a></td>
                  
                  <td width="10%">
                      <p id="<%=pastproperty._id%>" class="removeProperty">
                        <input type="hidden" id="t1-<%=pastproperty._id%>" value="<%=pastproperty.timestamp%>">
                        <i class="fa fa-times w3-margin-right links"></i>
                      </p>
                    </td>
                </tr>
              </tbody>
            </table>
            <%}) %>
         </div>
          </div>
        </div>      
      </div>
      <br>
      
      <!-- Interests --> 
      
      <br>
      <% messages.forEach(function(messages){%>
        <div class="w3-container w3-round w3-theme-l4 w3-border w3-theme-border w3-margin-bottom w3-hide-small">
          <span onclick="this.parentElement.style.display='none'" class="w3-hover-text-grey w3-closebtn">
            <i class="fa fa-remove"></i>
          </span>
          <p><strong><%=messages.subject%></strong></p>
          <p><%=messages.messageat%></p>
